{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "bio": {
        "type": "string"
      },
      "birthday": {
        "type": "string"
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      },
      "number": {
        "type": "string"
      }
    },
    "required": ["bio", "birthday", "image", "name", "number"]
  }
}
